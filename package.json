  "repository": {
    "type": "git",
    "url": "https://github.com/lrsjng/barejs.git"
  },
  "main": "dist/barejs",
  "scripts": {
    "lint": "eslint .",
    "test": "node test",
    "cover": "isparta cover --dir coverage --report text --report html test",
