    "precommit": "npm run -s check && npm run -s release"
  },
  "devDependencies": {
    "babel-loader": "6.4.1",
    "babel-preset-es2015": "6.24.1",
    "eslint": "3.19.0",
    "ghu": "0.12.0",
    "isparta": "4.0.0",
    "scar": "0.19.0"
